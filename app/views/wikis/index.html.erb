<h1> <div class="col-md-4">
  <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
</div> </h1>

  <div class="row">
    <h1 class="wiki-center col-md-8"> Public Wikis </h1>
    <div class="box-solid col-md-8">
      <% @wikis.each do |wiki| %>
        <div class="media wiki-shadow">
          <div class="media-body">
            <h4 class="media-heading">
              <%= link_to markdown(wiki.title), wiki %> <% if wiki.private?%> PRIVATE <% end %>
            </h4>
            <small>
              <%= markdown(wiki.body) %>
            </small>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
